{
  "name": "@enbock/time-tracker",
  "version": "0.2.0-dev.7",
  "homepage": "https://enbock.github.io/Time-Tracker/",
  "description": "Tracking start and end of working time.",
  "dependencies": {
    "@material/drawer": "*",
    "@material/select": "*",
    "autoprefixer": "^7.1.5",
    "axios": "^0.16.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.2.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-babel": "^7.0.2",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "fbjs": "^0.8.16",
    "file-loader": "^1.1.5",
    "fs-extra": "^4.0.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "^21.3.0-beta.2",
    "jest-enzyme": "^4.0.0",
    "object-assign": "^4.1.1",
    "prettier": "^1.8.2",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "url-loader": "^0.6.2",
    "webpack": "*",
    "webpack-dev-server": "*",
    "webpack-manifest-plugin": "*",
    "webpack-uglifyes-plugin": "^0.0.2",
    "whatwg-fetch": "^2.0.3",
    "yamljs": "^0.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest --env=jsdom --coverage --verbose",
    "test_alternate": "node scripts/test.js --env=jsdom --coverage",
    "deploy": "gh-pages -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/bootstrap.js",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/index.js",
      "!src/registerServiceWorker.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "packages\\babel"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
