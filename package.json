{
  "name": "@enbock/time-tracker",
  "version": "0.2.0-dev.9",
  "homepage": "https://enbock.github.io/Time-Tracker/",
  "description": "Experimental Project: Tracking start and end of working time.",
  "dependencies": {
    "gh-pages": "^2.2.0"
  },
  "scripts": {
    "start": "echo TODO",
    "build": "echo TODO",
    "test": "echo TODO #jest --env=jsdom --coverage --verbose",
    "deploy": "gh-pages -m 'auto commit [ci skip]' -d src"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/bootstrap.js",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/index.js"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*test.js"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$",
      "^.+\\.module\\.css$",
      "packages\\babel"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.css$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
