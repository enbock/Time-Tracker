{
  "name": "@enbock/time-tracker",
  "version": "0.2.0-dev.8",
  "homepage": "https://enbock.github.io/Time-Tracker/",
  "description": "Tracking start and end of working time.",
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@material/drawer": "0.28.0",
    "@material/select": "0.28.0",
    "@svgr/webpack": "2.4.1",
    "autoprefixer": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "~9.0.0",
    "babel-jest": "~24.5.0",
    "babel-loader": "~8.0.5",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-app": "^5.0.4",
    "babel-runtime": "^6.23.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.2.0",
    "css-loader": "^0.28.7",
    "dotenv": "^6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "~5.15.2",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "~2.1.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fbjs": "^0.8.16",
    "file-loader": "^3.0.1",
    "fs-extra": "^4.0.2",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jest": "^24.5.0",
    "jest-enzyme": "^7.0.2",
    "object-assign": "^4.1.1",
    "prettier": "^1.8.2",
    "promise": "^8.0.2",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^16.6.0",
    "react-app-polyfill": "~0.2.2",
    "react-dev-utils": "^6.0.5",
    "react-dom": "^16.6.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^0.6.2",
    "webpack": "^4.29.6",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.4",
    "whatwg-fetch": "^3.0.0",
    "yamljs": "^0.3.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest --env=jsdom --coverage --verbose",
    "test_alternate": "node scripts/test.js --env=jsdom --coverage",
    "deploy": "gh-pages -m 'auto commit [ci skip]' -d build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/bootstrap.js",
      "!src/index.js",
      "!src/setupTests.js",
      "!src/**/index.js",
      "!src/registerServiceWorker.js"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/*test.js"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$",
      "packages\\babel"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
