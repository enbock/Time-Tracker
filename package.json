{
  "name": "@enbock/time-tracker",
  "version": "0.2.0-dev.9",
  "homepage": "https://enbock.github.io/Time-Tracker/",
  "description": "Experimental Project: Tracking start and end of working time.",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/parser": "^2.17.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.18.0",
    "gh-pages": "^2.2.0",
    "jest": "^25.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "typescript": "^3.7.5",
    "babel-jest": "^25.1.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/jest": "^24.9.1",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@testing-library/react": "^9.4.0",
    "@testing-library/jest-dom": "^5.0.2"
  },
  "scripts": {
    "devServer": "powershell.exe -Command cd build; npx http-server",
    "start": "powershell.exe -Command bash scripts/copyFiles.sh develop; npx tsc --build tsconfig.json -v --watch",
    "test": "jest --coverage --verbose",
    "build": "sh scripts/build.sh",
    "deploy": "gh-pages -m 'auto commit [ci skip]' -d build"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/index.tsx"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)test)\\.tsx?$",
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "^.+\\.module\\.css$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 95
      }
    }
  }
}
